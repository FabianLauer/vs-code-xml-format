{
	"name": "XML Format",
	"description": "A simple XML formatter for VS Code.",
	"version": "0.0.1",
	"publisher": "Softwareschmiede Saar",
	"author": "Fabian Lauer",
	"engines": {
		"vscode": "^0.10.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.formatXml",
		"onLanguage:xml",
		"onLanguage:html"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.formatXml",
			"title": "Format XML"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	},
	"dependencies": {
		"xml-parser": "^1.2.1"
	}
}